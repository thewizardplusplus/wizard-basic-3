let ARRAY_SIZE = 5

function NumberArrayNewRandom(size)
	let array = new array(size)

	let i = 0
	while i < size do
		array[i] = GetRandom()
		i = i + 1
	end

	return array
end

function NumberArrayShow(array)
	Write(1, "[")

	let i = 0
	while i < GetLength(array) do
		if i > 0 then
			Write(1, ", ")
		end
		Write(1, ToString(array[i]))

		i = i + 1
	end

	Write(1, "]")
end

function Main(arguments)
	let array = NumberArrayNewRandom(ARRAY_SIZE)

	NumberArrayShow(array)
	Write(1, "\n")
end
