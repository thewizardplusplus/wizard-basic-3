include "array/number"

structure Reference
	value
end

function Ref(value)
	let reference = new Reference
	reference.value = value

	return reference
end

function Deref(reference)
	let value = NULL
	if Compare(GetType(reference), "Reference") == COMPARE_RESULT_EQUAL then
		value = reference.value
	else
		value = reference
	end

	return value
end
